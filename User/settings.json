// Place your settings in this file to overwrite the default settings
{
  "breadcrumbs.enabled": false,
  "debug.toolBarLocation": "docked",
  "editor.codeLens": false,
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.tokenColorCustomizations": {
    "comments": { "fontStyle": "" },
    "functions": { "fontStyle": "" },
    "keywords": { "fontStyle": "" },
    "numbers": { "fontStyle": "" },
    "strings": { "fontStyle": "" },
    "types": { "fontStyle": "" },
    "variables": { "fontStyle": "" },
    "textMateRules": [
      {
        "scope": [
          "entity.other.attribute-name",
          "markup.bold",
          "markup.underline.link.markdown",
          "markup.quote",
          "punctuation.definition.comment",
          "variable.language"
        ],
        "settings": { "fontStyle": "" }
      }
    ]
  },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "elixirLS.dialyzerEnabled": false,
  "explorer.openEditors.visible": 0,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": { "*.html.eex": "html" },
  "files.hotExit": "onExitAndWindowClose",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "html.format.wrapLineLength": 0,
  "material-icon-theme.activeIconPack": "none",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showReloadMessage": false,
  "material-icon-theme.showUpdateMessage": false,
  "npm.packageManager": "npm",
  "powershell.integratedConsole.showOnStartup": false,
  "powershell.powerShellExePath": "d:\\Apps\\PowerShell\\pwsh.exe",
  "powershell.sideBar.CommandExplorerVisibility": false,
  "search.smartCase": true,
  "terminal.external.windowsExec": "ConEmu64.exe",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.rendererType": "experimentalWebgl",
  "terminal.integrated.shell.windows": "pwsh.exe",
  "terminal.integrated.shell.linux": "zsh",
  "terminal.integrated.shellArgs.windows": ["--nologo"],
  "window.newWindowDimensions": "inherit",
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Material Theme",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "none",
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" }
}
